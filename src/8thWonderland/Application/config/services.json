{
    "database_connection" : {
        "class" : "Wonderland\\Library\\Database\\PdoDriver",
        "arguments" : [
            "%database_dsn%",
            "%database_username%",
            "%database_password%"
        ]
    },
    "session" : {
        "class" : "Wonderland\\Library\\Memory\\Session"
    },
    "error_handler" : {
        "class" : "Wonderland\\Library\\Security\\ErrorHandler",
        "arguments" : [
            "@database_logger",
            "@file_logger",
            "%error_reporting%"
        ]
    },
    "translator" : {
        "class" : "Wonderland\\Library\\Translator",
        "arguments" : [
            "%root_path%",
            "%language_options%"
        ]
    },
    "templating": {
        "class" : "Wonderland\\Library\\Templating\\Renderer",
        "arguments" : [
            "%root_path%",
            "@translator",
            "@abac"
        ]
    },
    "paginator" : {
        "class" : "Wonderland\\Library\\Plugin\\Paginator"
    },
    "database_logger" : {
        "class" : "Wonderland\\Library\\Logger\\DatabaseLogger",
        "arguments" : [
            "@database_connection"
        ]
    },
    "file_logger" : {
        "class" : "Wonderland\\Library\\Logger\\FileLogger",
        "arguments" : [
            "%root_path%"
        ]
    },
    "member_manager" : {
        "class" : "Wonderland\\Application\\Manager\\MemberManager",
        "arguments" : [
            "@member_repository",
            "@database_connection",
            "@country_manager",
            "@region_manager",
            "@translator"
        ]
    },
    "member_repository" : {
        "class" : "Wonderland\\Application\\Repository\\MemberRepository",
        "arguments" : [
            "@database_connection"
        ]
    },
    "country_manager" : {
        "class" : "Wonderland\\Application\\Manager\\CountryManager",
        "arguments" : [
            "@country_repository"
        ]
    },
    "country_repository" : {
        "class" : "Wonderland\\Application\\Repository\\CountryRepository",
        "arguments" : [
            "@database_connection"
        ]
    },
    "region_manager" : {
        "class" : "Wonderland\\Application\\Manager\\RegionManager",
        "arguments" : [
            "@region_repository"
        ]
    },
    "region_repository" : {
        "class" : "Wonderland\\Application\\Repository\\RegionRepository",
        "arguments" : [
            "@database_connection"
        ]
    },
    "group_manager" : {
        "class" : "Wonderland\\Application\\Manager\\GroupManager",
        "arguments" : [
            "@group_repository"
        ]
    },
    "mail_manager" : {
        "class" : "Wonderland\\Application\\Manager\\MailManager"
    },
    "facebook_manager" : {
        "class" : "Wonderland\\Application\\Manager\\FacebookManager",
        "arguments": [
            "%facebook_api_url%",
            "%facebook_app_id%",
            "%facebook_app_secret%",
            "%facebook_page_id%"
        ]
    },
    "ovh_manager" : {
        "class" : "Wonderland\\Application\\Manager\\OvhManager",
        "arguments": [
            "@logger"
        ]
    },
    "motion_manager" : {
        "class" : "Wonderland\\Application\\Manager\\MotionManager",
        "arguments" : [
            "@database_connection",
            "@translator",
            "@motion_repository"
        ]
    },
    "motion_repository" : {
        "class" : "Wonderland\\Application\\Repository\\MotionRepository",
        "arguments" : [
            "@database_connection"
        ]
    },
    "message_manager" : {
        "class" : "Wonderland\\Application\\Manager\\MessageManager",
        "arguments" : [
            "@database_connection",
            "@member_manager",
            "@message_repository"
        ]
    },
    "message_repository" : {
        "class" : "Wonderland\\Application\\Repository\\MessageRepository",
        "arguments" : [
            "@database_connection"
        ]
    },
    "group_repository" : {
        "class" : "Wonderland\\Application\\Repository\\GroupRepository",
        "arguments" : [
            "@database_connection"
        ]
    },
    "abac" : {
        "class" : "PhpAbac\\Abac",
        "arguments" : [
            "@database_connection"
        ]
    }
}
